 {% extends "layout.njk" %}

{% block content %}
  
<div id="wrapper">
<img src="{{data.avatar_url}}" alt="Pedro">
<h1>{{data.name}}</h1>
<h2>{{data.role}}</h2>
<p>{{data.discription}}</p>
<p>Fullstack development student</p>
</div>

<div id="links-footer" class=links>

{% for link in data.links %}

<a href="{{link.url}}"  target="_blank">{{link.name}}</a>

{% endfor %}

</div>

{% endblock content %}